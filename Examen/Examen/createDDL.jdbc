CREATE TABLE Abonne (cin INTEGER NOT NULL, nom VARCHAR(255), numAbonnement INTEGER, ouvrage VARCHAR(255), PRIMARY KEY (cin))
CREATE TABLE Ouvrage (titre VARCHAR(255) NOT NULL, DTYPE VARCHAR(31), dateCreation VARCHAR(255), exist TINYINT(1) default 0, editor VARCHAR(255), duree FLOAT, auteur VARCHAR(255), PRIMARY KEY (titre))
ALTER TABLE Abonne ADD CONSTRAINT FK_Abonne_ouvrage FOREIGN KEY (ouvrage) REFERENCES Ouvrage (titre)
CREATE TABLE SEQUENCE (SEQ_NAME VARCHAR(50) NOT NULL, SEQ_COUNT DECIMAL(38), PRIMARY KEY (SEQ_NAME))
